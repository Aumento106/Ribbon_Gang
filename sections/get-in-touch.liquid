              </svg>
              <label class="form-label" for="ContactName">{{ 'templates.contact.form.name' | t }} *</label>
            </div>
            <input 
              type="text" 
              class="form-input" 
              id="ContactName"
              name="contact[name]" 
              value="{{ form.name }}"
              required
